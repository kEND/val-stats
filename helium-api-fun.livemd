defmodule TailDkg do
  def elected?(lines) do
    case lines do
      [h | tail] ->
        if String.match?(h, ~r/not in DKG this round/) do
          IO.puts("NOT ELECTED: " <> String.trim(h))
        end

        if String.match?(h, ~r/Preparing to run DKG/) do
          IO.puts("ELECTED: " <> String.trim(h))
        end

        elected?(tail)

      [] ->
        []
    end
  end
end

