defmodule HeliumClient do
  require Tesla

  # helium-api.stakejoy.com
  def client(url \\ "https://api.helium.io/") do
    Tesla.client([
      {Tesla.Middleware.BaseUrl, url},
      Tesla.Middleware.JSON
    ])
  end
end
